<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] | She Wellness Co</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/swc.shewellness.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@300&family=League+Gothic&family=Lora&display=swap" rel="stylesheet">
</head>
<body>
    <header class="bg-shadow" style="z-index:999; position:fixed; width: 100%; border-radius:0px;" id="navigation-header">
        <nav class="h-25 d" id="navigation-color">
            <div class="d-flex">
                <div class="width-20 p-3 m-auto">
                    <a class="navbar-brand d text-white" asp-area="" asp-controller="Home" asp-action="Index">She Wellness Co</a>
                </div>
                    <div class="width-60 m-auto text-center p-3">
                        <ul class="navbar-nav flex-row d-flex m-auto text-center justify-content-center">
                            <li class="nav-item mx-4">
                                <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                            </li>
                            <li class="nav-item mx-4">
                                <a class="nav-link text-white" asp-area="" asp-controller="About" asp-action="Index">My Story</a>
                            </li>                        
                            <li class="nav-item mx-4">
                                  <a class="nav-link text-white" asp-area="" asp-controller="Contact" asp-action="Index">Get In Touch</a>
                            </li>
                        </ul>
                    </div>

                    <div class="width-20 p-3 text-right">
                        <div class="p-3 m-auto">
                            <a class="cta-button-wh p-3" asp-area="" asp-controller="Contact" asp-action="Index">Book Now</a>
                        </div>
                    </div>
            </div>
        </nav>
        @*Mobile menu + dropdown*@
        <div class="width-100">
            <partial name="_MobileMenu">
        </div>
    </header>


    @if (@ViewData["Title"] != "Home"){
        <div class="" id="body-padding-top"></div>
    }
    
    <div>
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>

    <footer class="d-flex width-100 bg-navy text-white p-3">
        <div class="p-2 width-50-fixed px-lg-5">
            <p>&copy; 2022 - She Wellness Co - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </p>
        </div>
        <div class="p-2 width-50-fixed text-right">
            <div>
                <p class="text-decoration-underline">Let's get social</p>
            </div>

            <div class="d-flex justify-content-end px-lg-5">
            <div class="width-10 my-auto text-right">
                    <a class="social-icons">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" fill="none">
                <circle cx="16" cy="16" r="14" fill="url(#paint0_linear_87_7208)" />
